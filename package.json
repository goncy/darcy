{
    "name": "styncy",
    "displayName": "Styncy",
    "description": "Stylish styles styled by Goncy, with variants for Atom dark one and Monokai pro, based on Atom IDE",
    "version": "0.0.8",
    "publisher": "goncy",
    "icon": "images/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/goncy/styncy.git"
    },
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Styncy",
                "uiTheme": "vs-dark",
